    "@remix-run/server-runtime": "^2.4.1",
    "@repo/ui": "*",
    "@vercel/analytics": "^1.1.1",
    "@vercel/remix-entry-server": "^0.1.1",
    "drizzle-orm": "^0.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "drizzle-kit": "^0.20.14",
    "typescript": "^5.3.3"
